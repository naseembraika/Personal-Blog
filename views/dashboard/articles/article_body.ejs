<div class="container">
    <h1 class="mb-4"><%= pageData.formDescription %></h1>
    <% if (locals.message) { %>
        <h4 class="mb-4" style="color: red;"><%= message %></h4>
    <% } %>

    <form action="<%= pageData.formLink %>" method="POST" enctype="multipart/form-data">
        <%- include('_form_fields') %>
    </form>
</div>